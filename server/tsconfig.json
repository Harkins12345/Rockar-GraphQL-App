  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext",
    "moduleResolution": "node",
    "sourceMap": true,
    "baseUrl": "src",
    "outDir": "dist",
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
  },
  "include": ["src/**/*"]
}
